{
    "name": "allcaretravel/laravel-videoable",
    "type": "library",
    "description": "This is a fork from nwidart/laravel-videoable and enhanced. Attach videos to your models",
    "keywords": [
        "laravel",
        "laravel-videoable",
        "video",
        "youtube",
        "vimeo"
    ],
    "homepage": "https://github.com/nwidart/laravel-videoable",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Widart",
            "email": "n.widart@gmail.com",
            "homepage": "https://nicolaswidart.com",
            "role": "Developer"
        },
        {
            "name": "Sophy SEM",
            "email": "sophy.s@allcaretravel.com",
            "homepage": "https://allcaretravel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4.0||^8.0.0",
        "laravel/framework": "^7.0.0||^8.0.0||^9.0.0"

    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.0",
        "squizlabs/php_codesniffer": "3.6.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "friendsofphp/php-cs-fixer": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "Nwidart\\LaravelVideoable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nwidart\\LaravelVideoable\\tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "fix-style": "vendor/bin/php-cs-fixer fix . --config-file='.php_cs' --verbose"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "laravel": {
            "providers": [
                "Nwidart\\LaravelVideoable\\LaravelVideoableServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
